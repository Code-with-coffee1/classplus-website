                                                                <h1>Assignment submission</h1>
                                                                       <!-- Main content -->
                                                            <form method="POST" action="/student_assignment" enctype="multipart/form-data">
                                                                <div>
                                                                <label for="file" >Choose file to upload</label><br/>
                                                                <input  class="btn btn-primary" type="button" id="upload-dialog" value="Choose PDF"/>
                                                                <input type="hidden" name="assignmentId" value="<%= pdf._id%>">
                                                                
                                                                <input  name="file" type="file" id="pdf-file" accept="application/pdf" style="display:none" />
                                                                <div id="pdf-loader" style="display:none">Loading Preview ..</div>
                                                                <canvas id="pdf-preview" width="150" style="display:none"></canvas> 
                                                            
                                                                </div>
